{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript",
    "plugin:prettier/recommended"
  ], 
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./client/tsconfig.json"
  }, 
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-key": "error",
    "@typescript-eslint/no-unused-vars": "off"
  }
}